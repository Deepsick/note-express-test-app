{
  "name": "@deepsick/note-app",
  "version": "1.0.0",
  "description": "REST Api for note-app",
  "main": "index.js",
  "keywords": [
    "api",
    "notes",
    "sharing"
  ],
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development npx nodemon index.js",
    "start": "NODE_ENV=production node index.js --production",
    "test::eslint": "npx eslint .",
    "test::editorconfig": "npx editorconfig-cli **/*.js",
    "test::jest": "NODE_ENV=testing npx jest tests",
    "test": "npm run test::editorconfig && npm run test::eslint && npm run test::jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepsick/note-express-test-app.git"
  },
  "jest": {
    "testTimeout": 10000
  },
  "author": "Deepsick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Deepsick/note-express-test-app/issues"
  },
  "homepage": "https://github.com/Deepsick/note-express-test-app#readme",
  "dependencies": {
    "bcrypt": "5.0.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-limiter": "1.6.1",
    "helmet": "4.2.0",
    "joi": "17.3.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "pg": "8.5.1",
    "pg-hstore": "2.3.3",
    "redis": "3.0.2",
    "sequelize": "6.3.5",
    "sqlite3": "5.0.0",
    "swagger-ui-express": "4.1.5",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "supertest": "6.0.1"
  }
}
